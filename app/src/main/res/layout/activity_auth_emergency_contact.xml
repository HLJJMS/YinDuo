<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:fitsSystemWindows="true">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/color_F4F4F4"
        tools:ignore="MissingConstraints">



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/width_16dp"
            android:layout_marginBottom="@dimen/width_39dp"
            android:gravity="center_horizontal"
            android:orientation="horizontal"
            app:layout_constraintTop_toBottomOf="@id/ok_bt">

            <ImageView
                android:layout_width="@dimen/width_12dp"
                android:layout_height="@dimen/width_15dp"
                android:layout_gravity="center_vertical"
                android:src="@mipmap/ic_security" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="5dp"
                android:singleLine="true"
                android:text="银行级数据加密服务"
                android:textColor="@color/color_ADADAD"
                android:textSize="13sp" />

        </LinearLayout>

        <TextView
            android:id="@+id/title1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="17dp"
            android:paddingTop="11dp"
            android:paddingBottom="11dp"
            android:text="直系亲属联系人"
            android:textColor="@color/color_9A9A9A"
            android:textSize="13dp"
            app:layout_constraintTop_toBottomOf="@id/titleBar" />

        <TextView
            android:id="@+id/contact"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:paddingStart="17dp"
            android:paddingTop="16dp"
            android:paddingBottom="16dp"
            android:text="与本人关系"
            android:textColor="@color/color_333333"
            android:textSize="14sp"
            app:layout_constraintTop_toBottomOf="@id/title1" />

        <TextView
            android:id="@+id/contact_txt"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:gravity="center"
            android:hint="请选择"
            android:textColor="@color/color_333333"
            android:textColorHint="#d0d0d0"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@id/contact"
            app:layout_constraintRight_toLeftOf="@id/contact_img"
            app:layout_constraintTop_toTopOf="@id/contact" />

        <TextView
            android:id="@+id/name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:paddingStart="17dp"
            android:paddingTop="16dp"
            android:paddingBottom="16dp"
            android:text="紧急联系人"
            android:textColor="@color/color_333333"
            android:textSize="14sp"
            app:layout_constraintTop_toBottomOf="@id/contact" />


        <TextView
            android:id="@+id/name_txt"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:gravity="center"
            android:hint="请选择"
            android:textColor="@color/color_333333"
            android:textColorHint="#d0d0d0"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@id/name"
            app:layout_constraintRight_toLeftOf="@id/name_img"
            app:layout_constraintTop_toTopOf="@id/name" />

        <TextView
            android:id="@+id/title2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="17dp"
            android:paddingTop="11dp"
            android:paddingBottom="11dp"
            android:text="其他联系人"
            android:textColor="@color/color_9A9A9A"
            android:textSize="13dp"
            app:layout_constraintTop_toBottomOf="@id/name" />

        <TextView
            android:id="@+id/contact_other"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:paddingStart="17dp"
            android:paddingTop="16dp"
            android:paddingBottom="16dp"
            android:text="与本人关系"
            android:textColor="@color/color_333333"
            android:textSize="14sp"
            app:layout_constraintTop_toBottomOf="@id/title2" />

        <TextView
            android:id="@+id/contact_other_txt"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:gravity="center"
            android:hint="请选择"
            android:textColor="@color/color_333333"
            android:textColorHint="#d0d0d0"

            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@id/contact_other"
            app:layout_constraintRight_toLeftOf="@id/contact_other_img"
            app:layout_constraintTop_toTopOf="@id/contact_other" />

        <TextView
            android:id="@+id/name_other"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:paddingStart="17dp"
            android:paddingTop="16dp"
            android:paddingBottom="16dp"
            android:text="紧急联系人"
            android:textColor="@color/color_333333"
            android:textSize="14sp"
            app:layout_constraintTop_toBottomOf="@id/contact_other" />

        <TextView
            android:id="@+id/name_other_txt"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:gravity="center"
            android:hint="请选择"
            android:textColor="@color/color_333333"
            android:textColorHint="#d0d0d0"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@id/name_other"
            app:layout_constraintRight_toLeftOf="@id/name_other_img"
            app:layout_constraintTop_toTopOf="@id/name_other" />






        <ImageView
            android:id="@+id/contact_img"
            android:layout_width="40dp"
            android:layout_height="30dp"
            android:paddingLeft="@dimen/width_10dp"
            android:paddingTop="@dimen/width_10dp"
            android:paddingRight="@dimen/width_15dp"
            android:paddingBottom="@dimen/width_10dp"
            android:src="@mipmap/ic_enter"
            app:layout_constraintBottom_toBottomOf="@id/contact"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/contact" />

        <ImageView
            android:id="@+id/name_img"
            android:layout_width="40dp"
            android:layout_height="30dp"
            android:paddingLeft="@dimen/width_10dp"
            android:paddingTop="@dimen/width_10dp"
            android:paddingRight="@dimen/width_15dp"
            android:paddingBottom="@dimen/width_10dp"
            android:src="@mipmap/ic_enter"
            app:layout_constraintBottom_toBottomOf="@id/name"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/name" />

        <ImageView
            android:id="@+id/contact_other_img"
            android:layout_width="40dp"
            android:layout_height="30dp"
            android:paddingLeft="@dimen/width_10dp"
            android:paddingTop="@dimen/width_10dp"
            android:paddingRight="@dimen/width_15dp"
            android:paddingBottom="@dimen/width_10dp"
            android:src="@mipmap/ic_enter"
            app:layout_constraintBottom_toBottomOf="@id/contact_other"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/contact_other" />

        <ImageView
            android:id="@+id/name_other_img"
            android:layout_width="40dp"
            android:layout_height="30dp"
            android:paddingLeft="@dimen/width_10dp"
            android:paddingTop="@dimen/width_10dp"
            android:paddingRight="@dimen/width_15dp"
            android:paddingBottom="@dimen/width_10dp"
            android:src="@mipmap/ic_enter"
            app:layout_constraintBottom_toBottomOf="@id/name_other"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/name_other" />

        <TextView
            android:id="@+id/ok_bt"
            style="@style/text_submit"
            android:layout_width="match_parent"
            android:layout_height="@dimen/width_45dp"
            android:layout_marginLeft="@dimen/width_20dp"
            android:layout_marginTop="@dimen/width_30dp"
            android:layout_marginRight="@dimen/width_20dp"
            android:enabled="false"
            android:text="保存"
            android:textSize="@dimen/size_16sp"
            app:layout_constraintTop_toBottomOf="@id/name_other" />

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginLeft="17dp"
            android:layout_marginRight="17dp"
            android:background="@color/color_EAEAEA"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/contact" />

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginLeft="17dp"
            android:layout_marginRight="17dp"
            android:background="@color/color_EAEAEA"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/contact_other" />

        <com.yinduowang.installment.app.widget.TitleBar
            android:id="@+id/titleBar"
            android:layout_width="match_parent"
            android:layout_height="?attr/qmui_topbar_height" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>
